{
    "name": "myvendor/my-rest-api",
    "description": "A simple REST API using Slim and PostgreSQL",
    "type": "project",
    "require": {
        "illuminate/database": "^9.0",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.10",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
